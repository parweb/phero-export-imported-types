{
  "name": "typescript-node",
  "version": "1.0.0",
  "description": "Basic demo of using Typescript on the server side",
  "main": "dist/my-lib.js",
  "module": "dist/my-lib.mjs",
  "typings": "dist/my-lib.d.ts",
  "scripts": {
    "export": "tsc --emitDeclarationOnly --outDir dist && dts-bundle --name=generated --main=dist/sample/index.d.ts",
    "export-all": "npm-dts generate"
  },
  "dependencies": {
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  },
  "devDependencies": {
    "@types/node": "18.11.5",
    "dts-bundle": "0.7.3",
    "nodemon": "2.0.20",
    "npm-dts": "1.3.12"
  },
  "keywords": []
}
